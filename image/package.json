{
    "name": "@magenta/image",
    "version": "0.2.1",
    "description": "Visual art with machine learning, in the browser.",
    "main": "es5/index.js",
    "types": "es5/index.d.ts",
    "jsdelivr": "dist/magentaimage.js",
    "unpkg": "dist/magentaimage.js",
    "dependencies": {
        "@tensorflow/tfjs": "^1.1.2"
    },
    "devDependencies": {
        "@types/file-saver": "^1.3.0",
        "clang-format": "^1.2.3",
        "file-saver": "^1.3.8",
        "file-saver-typescript": "^1.0.1",
        "http-server": "^0.11.1",
        "in-publish": "^2.0.0",
        "minimist": "^1.2.0",
        "path": "^0.12.7",
        "ts-node": "^5.0.1",
        "ts-loader": "^5.3.0",
        "tsify": "^3.0.4",
        "tslint": "^5.9.1",
        "typedoc": "^0.14.2",
        "typedoc-plugin-sourcefile-url": "^1.0.3",
        "typescript": "^3.3.3333",
        "webpack": "^4.24.0",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.14"
    },
    "scripts": {
        "prepublish": "in-publish && yarn lint && yarn build && yarn docs && yarn bundle || not-in-publish",
        "build": "tsc",
        "bundle": "webpack --config ./webpack/lib.config.ts",
        "lint": "tslint -c ../tslint.json -p . -t verbose",
        "build-demos": "webpack --config ./webpack/demo.config.ts",
        "run-demos": "yarn build-demos && http-server demos/",
        "publish-demos": "yarn build-demos && mkdir -p ../docs/image/demos && cp demos/*.{js,html,css,jpg} ../docs/image/demos",
        "serve-demos": "http-server demos/",
        "serve-dist": "http-server dist/",
        "docs": "sh ./scripts/generate-docs.sh && yarn publish-demos"
    },
    "author": "Magenta",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/tensorflow/magenta-js.git"
    }
}
