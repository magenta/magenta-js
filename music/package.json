{
  "name": "@magenta/music",
  "version": "1.9.0",
  "description": "Make music with machine learning, in the browser.",
  "main": "es5/index.js",
  "module": "es6/index.js",
  "types": "es5/index.d.ts",
  "jsdelivr": "dist/magentamusic.js",
  "unpkg": "dist/magentamusic.js",
  "dependencies": {
    "@tensorflow/tfjs": "^1.1.2",
    "fft.js": "^4.0.3",
    "midiconvert": "^0.4.7",
    "ndarray-resample": "^1.0.1",
    "protobufjs": "^6.8.6",
    "tonal": "^2.0.0",
    "tone": "^13.8.21"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@types/clone": "^0.1.30",
    "@types/d3": "^5.7.0",
    "@types/file-saver": "^1.3.0",
    "@types/jasmine": "^3.4.0",
    "@types/ndarray": "^1.0.6",
    "@types/tape": "^4.2.32",
    "@types/webmidi": "^2.0.2",
    "audio-recorder-polyfill": "^0.1.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "browserify": "^14.4.0",
    "clang-format": "^1.2.3",
    "clone": "^1.0.4",
    "d3": "^5.8.0",
    "file-saver": "^1.3.8",
    "file-saver-typescript": "^1.0.1",
    "http-server": "^0.11.1",
    "in-publish": "^2.0.0",
    "jasmine": "^3.4.0",
    "jasmine-ts": "^0.3.0",
    "microbundle": "^0.12.0-next.4",
    "minimist": "^1.2.0",
    "tape": "^4.9.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^5.3.0",
    "ts-node": "^8.3.0",
    "tsify": "^3.0.4",
    "tslint": "^5.9.1",
    "typedoc": "^0.14.2",
    "typedoc-plugin-sourcefile-url": "^1.0.3",
    "typescript": "^3.5.3",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "scripts": {
    "prepublish": "in-publish && yarn lint && yarn test && yarn build && yarn docs && yarn bundle || not-in-publish",
    "build:es5": "tsc --outdir es5 --module commonjs --target es5 && cp src/protobuf/proto.* es5/protobuf",
    "build:es6": "webpack --config webpack/es6.config.js && cp src/protobuf/proto.* es6/protobuf",
    "xx-build:es6": "microbundle -f es -o es6/core.js src/core.ts --external none && cp src/protobuf/proto.* es6/protobuf",
    "x-build:es6": "microbundle -f es -o es6/core.js src/core.ts --external tone.js,tone,midiconvert && cp src/protobuf/proto.* es6/protobuf",
    "build": "npm run build:es5 && npm run build:es6",
    "bundle": "webpack --config ./webpack/lib.config.ts",
    "lint": "tslint -c ../tslint.json -p . -t verbose",
    "test-old": "ts-node node_modules/tape/bin/tape src/**/*_test.ts",
    "test": "jasmine-ts --config=jasmine.json",
    "build-demos": "webpack --config ./webpack/demo.config.ts",
    "publish-demos": "yarn build-demos && mkdir -p ../docs/music/demos && cp demos/*.{js,html,mid,css} ../docs/music/demos",
    "run-demos": "webpack-dev-server --config ./webpack/demo.config.ts",
    "serve-demos": "http-server demos/",
    "serve-dist": "http-server dist/",
    "proto": "sh ./scripts/compile-proto.sh",
    "docs": "sh ./scripts/generate-docs.sh && yarn publish-demos"
  },
  "author": "Magenta",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tensorflow/magenta-js.git"
  }
}
